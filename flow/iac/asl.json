{
    "StartAt": "Ingest",
    "States": {
        "Ingest": {
            "Type": "Map",
            "ItemsPath": "$.input",
            "Iterator": {
                "StartAt": "SIngest",
                "States": {
                    "SIngest": {
                        "Type": "Task",
                        "Resource": "${FnIngest}",
                        "OutputPath": "$",
                        "End": true
                    }
                }
            },
            "End": true
        }
    }
}